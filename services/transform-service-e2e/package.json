{
  "name": "@rating-platform/transform-service-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "transform-service-e2e",
    "implicitDependencies": [
      "transform-service"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "services/transform-service-e2e/jest.config.cts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "transform-service:build",
          "transform-service:serve"
        ]
      }
    }
  }
}
